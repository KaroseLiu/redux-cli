{
  "name": "redux-cli",
  "version": "0.1.0",
  "description": "An opinionated CLI to make working on redux apps much faster.",
  "main": "bin/index.js",
  "scripts": {
    "test": "npm run lint && env NODE_PATH=$NODE_PATH:$PWD/src ./node_modules/.bin/babel-node node_modules/isparta/bin/isparta cover --report html ./node_modules/.bin/_mocha -- --compilers js:babel-register --opts ./.mocha.opts",
    "start": "./node_modules/.bin/babel src --watch -d lib",
    "lint": "./node_modules/.bin/eslint ./src ./test",
    "cover": "./node_modules/.bin/istanbul cover"
  },
  "keywords": [
    "redux",
    "react",
    "cli"
  ],
  "author": "Spencer Dixon",
  "license": "MIT",
  "bin": {
    "redux": "bin/index.js",
    "redux-init": "bin/redux-init.js",
    "redux-dumb": "bin/redux-dumb.js",
    "redux-smart": "bin/redux-smart.js",
    "redux-form": "bin/redux-form.js",
    "redux-duck": "bin/redux-duck.js"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "ejs": "^2.4.1",
    "fs-extra": "^0.26.5",
    "jsonfile": "^2.2.3",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SpencerCDixon/redux-cli"
  },
  "bugs": {
    "url": "https://github.com/SpencerCDixon/redux-cli/issues"
  }
}
